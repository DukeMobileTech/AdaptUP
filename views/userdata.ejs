<% include header.html %>

<div class="col-sm-2 pull-right">
  <a href="/logout" class="btn btn-primary" id="adaptup-logout">Log out</a>
</div>

<div class="row">
  <h1>Jawbone Data For <%= user %></h1>
  <table class="sleep-table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Amount of Sleep</th>
        <th>Number of Steps Taken</th>
        <th>Resting HeartRate</th>
      </tr>
    </thead>
    <tbody>
      
      <% for (var i = 0; i < items.length; i++) { %>
        <tr>
          <td><%= items[i].date %></td>
          <td><%= items[i].sleep_duration %></td>
          <td><%= items[i].step_count %></td>
          <td><%= items[i].resting_heartrate %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  
<% include footer.html %>